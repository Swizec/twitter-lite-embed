{
    "name": "twitter-lite-embed",
    "version": "1.0.0-0",
    "description": "The fastest most privacy focused Twitter embed component.",
    "repository": "git@github.com:Swizec/twitter-lite-embed.git",
    "author": "Swizec Teller <swizec@swizec.com>",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "start": "yarn build && yarn web-dev-server --open demo/ --node-resolve",
        "start:watch": "concurrently --raw \"yarn tsc --watch --preserveWatchOutput\" \"yarn wds --watch --open /demo --node-resolve\""
    },
    "files": [
        "dist/*"
    ],
    "main": "dist/twitter-lite-embed.js",
    "module": "dist/twitter-lite-embed.js",
    "types": "dist/twitter-lite-embed.d.ts",
    "keywords": [
        "web components",
        "twitter"
    ],
    "devDependencies": {
        "@web/dev-server": "^0.1.34",
        "concurrently": "^7.3.0",
        "dotenv": "^16.0.2",
        "koa-proxies": "^0.12.2",
        "twitter-lite": "^1.1.0",
        "typescript": "^4.8.2"
    },
    "dependencies": {}
}
